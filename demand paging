class demandpaging
{
// pfst=page fault service time
// e=eat
// mat=memory access time
// pfr=page fault rate
	public static void main(String args[]){
		double pfst=8;
		double e=200;
		double mat=200;
		double pfr;
		pfst=(0.7)*20+0.3*8;
                pfr=((e-100)/pfst)*1/1000000;
		//pfr=((e-100)/pfst)*Math.pow(10,-6);
		System.out.println("Required page fault rate="+pfr);
	}
}



